<!DOCTYPE html>
<html>
<head>
  <title>Test14</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <%= link_to "投稿画像", pictures_path, class: "btn btn-default btn-primary btn-sm" %>
    <% unless user_signed_in? %>
      <%= link_to "サインイン", new_user_session_path, class: "btn btn-default btn-success btn-sm" %>
      <%= link_to "サインアップ", new_user_registration_path, class: "btn btn-default btn-success btn-sm" %>
    <% else %>
      <%= link_to "ユーザー一覧", users_path, class: "btn btn-default btn-success btn-sm" %>
      <%= link_to "マイページ", user_path(current_user.id), class: "btn btn-default btn-success btn-sm" %>
      <%= link_to "ユーザー情報更新", edit_user_registration_path, class: "btn btn-default btn-success btn-sm" %>
      <%= link_to "サインアウト", destroy_user_session_path, method: :delete, class: "btn btn-default btn-danger btn-sm" %>
    <% end %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
  </header>
  <div class="container">
    <div class="wrapper col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1">

<%= yield %>

    </div>
  </div>
  <div id="page-top-btn" class="page-top-btn">
    <p><a class="move-page-top" id="move-page-top">▲</a></p>
  </div>
  <div class="bottom-spacer"></div>
  <footer>
  <p>山© 2015-2016 Copyright Yama Corp.</p>
  </footer>

</body>
</html>
